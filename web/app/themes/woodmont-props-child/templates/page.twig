{% extends "base.twig" %}

{% block content %}
	
	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1">{{post.title}}</h1>
				<div class="article-body">
					{{post.content}}
				</div>
			</section>
			<section class="section__staff">
				<h2 class="section__headline">Who We Are</h2>
				{% include "partials/staff.twig" %}
			</section>
			<section class="section__properties">
				<h2 class="section__headline">Properties</h2>
				{% include "partials/properties.twig" %}
			</section>
		</article>
	</div><!-- /content-wrapper -->

{% endblock %}