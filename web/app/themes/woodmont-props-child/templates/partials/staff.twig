{% for member in staff %}
	<img class="staff-member__portrait" src="{{ member.thumbnail }}" height="200" style="float:left;margin-right:15px;">
	<h3 class="staff-member__name">{{ member.title }}</h3>
	<h4 class="staff-member__job-title">{{ member.job_title }}</h4>
	<p class="staff-member__description">{{ member.content }}</p>
{% endfor %}